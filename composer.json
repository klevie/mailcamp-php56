{
  "name": "klevie/mailcamp-php56",
  "type": "library",
  "description": "Mailcamp php library, fork of seacommerce/mailcamp",
  "config": {
    "platform": {
      "php": "5.6"
    },
    "sort-packages": true
  },
  "license": "MIT",
  "require": {
    "php": "^5.6",
    "symfony/property-access": "^3.4",
    "symfony/serializer": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "symfony/dotenv": "^3.4"
  },
  "conflict": {
  },
  "suggest": {
  },
  "autoload": {
    "psr-4": {
      "Seacommerce\\Mailcamp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Seacommerce\\Mailcamp\\Tests\\": "tests/"
    }
  }
}
